{
  "name": "@helperai/nextjs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "postinstall": "fumadocs-mdx",
    "build": "webpack --config webpack.sdk.cjs --env production && next build",
    "build:sdk": "webpack --config webpack.sdk.cjs --env development",
    "build:sdk:prod": "webpack --config webpack.sdk.cjs --env production",
    "build:preview": "turbo run build && npm run db:migrate && npx tsx --conditions=react-server ./db/seeds/index.ts",
    "clean": "git clean -xdf .cache .next .turbo node_modules",
    "deploy": "turbo run build && npm run db:prod:migrate",
    "dev": "npx next dev --turbopack -H 0.0.0.0",
    "dev:sdk": "webpack --config webpack.sdk.cjs --watch",
    "dev:email": "npm run with-dev-env email dev -- --dir emails --port 3060",
    "format": "prettier --check . --ignore-path ./.gitignore --ignore-path ./.prettierignore",
    "db:check": "npm run with-dev-env drizzle-kit check -- --config ./db/drizzle.config.ts",
    "db:generate": "npm run with-dev-env drizzle-kit generate -- --config ./db/drizzle.config.ts",
    "db:migrate": "npm run with-dev-env drizzle-kit migrate -- --config ./db/drizzle.config.ts",
    "db:prod:migrate": "drizzle-kit migrate --config ./db/drizzle.config.ts",
    "db:reset": "npm run with-dev-env -- npx tsx --conditions=react-server ./db/reset.ts",
    "eval:dev": "EVALITE=true NODE_ENV=test npm run with-dev-env evalite watch",
    "evals": "EVALITE=true NODE_ENV=test npm run with-dev-env evalite",
    "lint": "eslint",
    "repl": "npm run with-dev-env -- npx tsx --conditions=react-server --import replGlobals.mts",
    "repl:preview": "sh ./with-vercel-env.sh preview npx tsx --conditions=react-server --import replGlobals.mts",
    "repl:prod": "sh ./with-vercel-env.sh production npx tsx --conditions=react-server --import replGlobals.mts",
    "start": "npx next start",
    "test": "npm run with-dev-env vitest",
    "test:watch": "npm run with-dev-env vitest -- --watch",
    "typecheck": "npm run with-dev-env npm run build; BUILD_EXIT=$?; tsc --noEmit && ([ $BUILD_EXIT -eq 0 ] && npm run db:check || exit $BUILD_EXIT)",
    "with-dev-env": "dotenv -e ./.env.development.local -e ./.env.local --"
  },
